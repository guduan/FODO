&run_setup
        lattice = fodo.lte,
        use_beamline =test
        final = %s.fin,
		output = %s.out,
        sigma = %s.sig,
        centroid = %s.cen,
        p_central_mev = 1000,
		magnets=%s.mag
		parameters = %s.param
		always_change_p0 =1
&end

&load_parameters
    filename = quad_K1_1000MeV.sdds
	change_defined_values = 1
	force_occurence_data=1
&end

&run_control
        n_steps = 1
&end


&error_control  error_log = %s.erl &end

! Quad misalignment
&error_element name=Q*, item=DX, amplitude=200e-6, cutoff=3, bind=0 &end
&error_element name=Q*, item=DY, amplitude=200e-6, cutoff=3, bind=0 &end

! BPM misalignment		
&error_element name=m1 element_type=MONI item=DX amplitude=200e-6 cutoff=3 bind=0 &end
&error_element name=m1 element_type=MONI item=DY amplitude=200e-6 cutoff=3 bind=0 &end

!! Incoming misalignment		
&error_element name=MALIN element_type=MALIGN item=DX amplitude=0e-6 cutoff=3 bind=0 &end
&error_element name=MALIN element_type=MALIGN item=DXP amplitude=10e-6 cutoff=3 bind=0 &end


&twiss_output
    filename = %s.twi
    matched = 0,  
    output_at_each_step=0, 
	beta_x = 16.3093  
	beta_y = 4.06314
&end

&matrix_output
	printout=%s.mpr
    SDDS_output = %s.mat
    SDDS_output_order = 1
	individual_matrices=1
	output_at_each_step=1
&end

&bunched_beam
	n_particles_per_bunch = 1
	use_twiss_command_values = 1
&end

&track &end
